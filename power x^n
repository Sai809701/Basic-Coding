import java.util.*;
class Scratch {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter base and index respectively:");
        int x = sc.nextInt();
        int n = sc.nextInt();
        int result = 1;
        for (int i =0; i<n; i++) {
            result = result*x;
        }
        System.out.println(result);
    }
}
